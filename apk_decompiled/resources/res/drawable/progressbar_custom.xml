<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#b5b5b5" android:endColor="#b5b5b5" android:angle="270" android:centerY="0.75" android:centerColor="#b5b5b5"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#b5b5b5" android:endColor="#b5b5b5" android:angle="270" android:centerY="0.75" android:centerColor="#b5b5b5"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#5fbef7" android:endColor="#1e70ba" android:angle="270" android:centerY="0.75" android:centerColor="#3b97dc"/>
                <stroke android:width="3dp" color="#000000"/>
                <padding android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
